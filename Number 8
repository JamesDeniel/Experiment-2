#include <iostream>
#include <conio.h>
using namespace std;

int main()
{
    int a;

    cout << "Enter a positive integer: ";

    cin >> a;
    cout << "Multiplication table of: " << a << endl;
    for (int b = 1; b <= 10; ++b) {
        cout << a << " * " << b << " = " << a * b << endl;
    }
    _getch();
    return 0;
}
